<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AndroidApp.LockedAcions"
             Title="Locked actions">
    <ScrollView>
        <StackLayout Orientation="Vertical">
            <Button x:Name="btnStopFiltering" Text="Stop filtering" ></Button>
            <Button x:Name="btnStopService" Text="Stop Service"></Button>
            <Button x:Name="btnSetMasterPass" Text="Set unlock password" ></Button>
            <Button x:Name="btnLock" Text="Lock To Date" Clicked="BtnLock_Clicked"></Button>


            <Button x:Name="btnImportPolicies" Text="Import 3 Policies" ></Button>
            <Label Text="URL, Time and Wifis" 
                   HorizontalOptions="Center" />
        </StackLayout>
    </ScrollView>
</ContentPage>